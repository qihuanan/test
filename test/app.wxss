/**app.wxss**/
@import './pages/style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  
} 

.pretabbar{
  height: 10rpx;
  margin-bottom: 100rpx;
}
.weui-tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  margin-top: 20rpx;
}
.weui-tabbar-img {
  height: 30px;
  width: 30px;
}
.weui-tabbar-img-2 {
  height: 30px;
  width:17px;
}